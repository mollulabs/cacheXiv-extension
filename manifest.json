{
    "manifest_version": 3,
    "name": "cacheXiv extention",
    "version": "1.0",
    "description": "for faster arXiv",
    "icons": {
        "32": "assets/icon-32.png"
    },
    "content_scripts": [
        {
            "js": ["scripts/content.js"],
            "matches": [
                "*://arxiv.org/pdf/*"
            ]
        }
    ]
}